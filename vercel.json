{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/watch",
      "dest": "api/bot.py"
    },
    {
      "src": "/unwatch",
      "dest": "api/bot.py"
    },
    {
      "src": "/list",
      "dest": "api/bot.py"
    }
  ]
}

